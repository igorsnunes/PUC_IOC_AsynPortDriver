record(ao, "address")
{
   field(PINI, "1")
   field(DTYP, "asynInt32")
   field(OUT, "@asyn($(PORT), 0, 5)ADDRESS")
   field(VAL, "$(ADDRESS_VAL)")
   field(SCAN, "Passive")
}

