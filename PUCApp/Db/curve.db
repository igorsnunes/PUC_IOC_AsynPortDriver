}

record(waveform, "value")
{
    field(PINI, "1")
    field(DTYP, "asynFloat64ArrayIn")
    field(INP,  "@asyn($(PORT),0,5)VALUE")
    field(FTVL, "DOUBLE")
    field(NELM, "16300")
    field(SCAN, "1 second")
}

